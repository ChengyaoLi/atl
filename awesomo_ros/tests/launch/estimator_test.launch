<?xml version="1.0"?>
<launch>
  <!-- settings -->
  <param name="use_sim_time" value="false" />

  <!-- target node -->
  <param name="sim_mode" value="false" />
  <param name="debug_mode" value="false" />
  <param name="estimator_config" value="$(find awesomo_configs)/configs/estimator/kf_tracker.yaml" />
  <node pkg="awesomo_ros" name="awesomo_estimator" type="estimator_node" output="screen" />

  <!-- test node -->
  <test pkg="awesomo_ros" test-name="awesomo_estimator_test" type="estimator_test" cwd="node"/>
</launch>
